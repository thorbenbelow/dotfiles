# Reload yabai
cmd + alt + shift - r : yabai --restart-service

# Move focus
cmd + alt - h : yabai -m window --focus west
cmd + alt - j : yabai -m window --focus south
cmd + alt - k : yabai -m window --focus north
cmd + alt - l : yabai -m window --focus east

# Move window
cmd + alt + shift - h : yabai -m window --warp west
cmd + alt + shift - j : yabai -m window --warp south
cmd + alt + shift - k : yabai -m window --warp north
cmd + alt + shift - l : yabai -m window --warp east

# Resize window
cmd + alt + ctrl - h : yabai -m window --resize left:-20:0
cmd + alt + ctrl - l : yabai -m window --resize right:20:0
cmd + alt + ctrl - k : yabai -m window --resize top:0:-20
cmd + alt + ctrl - j : yabai -m window --resize bottom:0:20

# Toggle float mode
cmd + alt - t : yabai -m window --toggle float

